<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:draw="clr-namespace:MauiChartApp.Component"
             x:Class="MauiChartApp.Component.ChartView" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
    <ContentView.Resources>
        <ResourceDictionary>
            <draw:RandomLines x:Key="RandomLines"></draw:RandomLines>
        </ResourceDictionary>
    </ContentView.Resources>

    <GraphicsView x:Name="graphicsView" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"></GraphicsView>

</ContentView>
